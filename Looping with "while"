#include <cstdio> //useful library, no need for iostream
using namespace std;
int main( int argc, char ** argv ) //main function returning values for cstdio library
{
  int a[] = {1,2,3,4,5};
  int i = 0;
  while (i<5) {
    printf("element %d is %d\n", i, a[i]); //printf = cout in cstdio library
    i++;
    }
  return 0;
}
